@model MyWebProjection.Models.Personal
@{
    ViewBag.Title = "Добавление пользователя";
}
<form asp-action="create" asp-controller="home">
    <div class="form-group">
        <label asp-for="FirstName" class="btn-close">First Name</label>
        <input type="text" asp-for="FirstName" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="LastName" class="btn-close">LastName</label>
        <input type="text" asp-for="LastName" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="MiddleName" class="btn-close">MiddleName</label>
        <input type="text" asp-for="MiddleName" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-success" />
    </div>
</form>
